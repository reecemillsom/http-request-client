{
  "name": "http-request-client",
  "version": "3.2.0",
  "description": "Http Client wrapper for doing requests",
  "main": "./dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "rm -rf dist/ && tslint -c tslint.json src/**/*.ts",
    "test": "npm run tslint && npm run tsc && nyc mocha",
    "browserify": "browserify ./scripts/index.js -p [ tsify ] -s requests -o ./browserify/TestRequests.js"
  },
  "author": "Reece Millsom",
  "license": "ISC",
  "keywords": [
    "http",
    "request",
    "httpclient",
    "get",
    "put",
    "patch",
    "post",
    "delete",
    "head",
    "fetch"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.132",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.12",
    "@types/sinon-chai": "^3.2.2",
    "browserify": "^16.3.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.2.0",
    "tsify": "^4.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "lodash": "^4.17.11"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 90,
    "branches": 90,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "check-coverage": true,
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./coverage"
  }
}
